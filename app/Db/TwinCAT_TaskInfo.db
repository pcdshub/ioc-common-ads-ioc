
record(longin,"$(PREFIX):TaskInfo:$(IDX):CycleTime"){
 field(PINI, "1")
 field(TSE, -2)
 field(DTYP, "asynInt32")
 field(INP,  "@asyn($(PORT),0,1)ADSPORT=851/POLL_RATE=1/TwinCAT_SystemInfoVarList._TaskInfo[$(IDX)].CycleTime?")
 field(SCAN,"I/O Intr")
 info(archive, "monitor 1: VAL")
}

